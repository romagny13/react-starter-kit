{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev --open --inline --hot",
    "test": "jest",
    "lint": "esw src --color",
    "prebuild:dist": "rimraf dist",
    "build:dist": "webpack --progress --hide-modules && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build:commonjs": "babel ./src/ --out-dir ./lib/lib",
    "build:copy-files": "node scripts/copy-files.js && cpx \"./src/*.css\" ./lib/lib",
    "prebuild:lib": "rimraf lib",
    "build:lib": "npm run build:dist && npm run build:commonjs && npm run build:copy-files"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "^3.1.2",
    "fs-extra": "^4.0.0",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js"
    }
  }
}
