{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev --open --inline --hot",
    "test": "jest",
    "lint": "esw src --color",
    "prebuild:dist": "rimraf dist",
    "build:dist": "webpack --progress --hide-modules && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build:commonjs": "babel ./src/ --out-dir ./lib/lib",
    "build:copy-files": "node scripts/copy-files.js && cpx \"./src/*.css\" ./lib/lib",
    "prebuild:lib": "rimraf lib",
    "build:lib": "npm run build:commonjs && npm run build:copy-files"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.10.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.3",
    "fs-extra": "^4.0.2",
    "jest": "^21.2.1",
    "react-test-renderer": "^16.1.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0"
  }
}
